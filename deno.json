{
  "name": "@nullstyle/ase-format",
  "version": "0.1.0",
  "exports": "./mod.ts",
  "tasks": {
    "check": "deno check mod.ts",
    "test": "deno test -A",
    "lint": "deno lint",
    "fmt": "deno fmt",
    "doc:lint": "deno doc --lint mod.ts",
    "doc:html": "deno doc --html mod.ts"
  },
  "imports": {
    "@std/bytes": "jsr:@std/bytes@^1",
    "@std/io": "jsr:@std/io@^0.225",
    "@std/streams": "jsr:@std/streams@^1",
    "@std/assert": "jsr:@std/assert@^1"
  },
  "compilerOptions": {
    "strict": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true
  },
  "exclude": [
    "vendor/"
  ],
  "fmt": {
    "exclude": ["vendor/"]
  },
  "lint": {
    "exclude": ["vendor/"]
  }
}
